# Honcho Configuration
HONCHO_API_KEY=your-honcho-api-key-here
HONCHO_BASE_URL=https://api.honcho.dev
HONCHO_WORKSPACE_ID=default

# Agent Configuration
CHAT_SERVER=
ENABLE_DEMO_AGENTS=true  # Set to false to disable auto-starting demo agents

# AI Model Configuration
# Option A: Use OpenRouter
USE_OPENROUTER=true
MODEL=openai/gpt-4o-mini
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Option B: Use Local Ollama
# USE_OPENROUTER=false
# MODEL=llama3.1:8b
# OPENROUTER_API_KEY is not needed for Ollama
